#include <stdio.h>
#include <stdlib.h>

int main() {
	
    int sayi,islem;
    
    printf("Bir sayi giriniz: ");
    scanf("%d",&sayi);
    
    if(++sayi%2==0)
    {
    	printf("cift\n");
    	
    	if(sayi<100)
    	{
    		islem=sayi*2;
		}
		else
		{
			islem=sayi+40;
		}
	}
    else if(sayi--%5==0)
    {
    	printf("5 e bolunuyor\n");
    	
    	if(sayi%3)
    	{
    		islem=sayi/2;
		}
	}
	printf("sayi %d",islem);
	
	return 0;
}

